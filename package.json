    "release:foodiez": "pnpm run bulid:frontend && pnpm run deploy:frontend"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "workspaces": [
    "packages/contracts",
    "apps/foodiez"
  ]
}
