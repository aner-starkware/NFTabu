{
  "name": "catering-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev:foodiez": "pnpm --filter @catering-app/foodiez dev",
    "build:foodiez": "pnpm --filter @catering-app/foodiez run build",
